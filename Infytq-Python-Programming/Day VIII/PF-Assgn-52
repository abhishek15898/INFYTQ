    sum_of_elements=0
    for items in sample_data:
        sum_of_elements+=items
    return sum_of_elements 
    

def even(data):
    even_list=0
    for e1 in sample_data:
        if(e1%2==0):
            even_list+=e1
    return even_list

def odd(data):
    odd_list=0
    for o in sample_data:
        if(o%2!=0):
            odd_list+=o1
    return odd_list

sample_data = range(1,11)

print(sum_of_numbers(sample_data,None))
